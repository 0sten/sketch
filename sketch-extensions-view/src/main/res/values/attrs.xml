<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="sketch_depth" format="enum">
        <enum name="network" value="1" />
        <enum name="local" value="2" />
        <enum name="memory" value="3" />
    </attr>

    <attr name="sketch_downloadCachePolicy" format="enum">
        <enum name="enabled" value="1" />
        <enum name="readOnly" value="2" />
        <enum name="writeOnly" value="3" />
        <enum name="disabled" value="4" />
    </attr>

    <attr name="sketch_preferQualityOverSpeed" format="boolean|reference" />
    <attr name="sketch_colorType" format="enum">
        <enum name="lowQuality" value="1" />
        <enum name="highQuality" value="2" />
        <enum name="alpha8" value="3" />
        <enum name="rgb565" value="4" />
        <enum name="argb4444" value="5" />
        <enum name="argb8888" value="6" />
        <enum name="rgbaF16" value="7" />
        <enum name="hardware" value="8" />
    </attr>
    <attr name="sketch_colorSpace" format="enum">
        <enum name="srgb" value="1" />
        <enum name="linearSrgb" value="2" />
        <enum name="extendedSrgb" value="3" />
        <enum name="linearExtendedSrgb" value="4" />
        <enum name="bt709" value="5" />
        <enum name="bt2020" value="6" />
        <enum name="dciP3" value="7" />
        <enum name="displayP3" value="8" />
        <enum name="ntsc1953" value="9" />
        <enum name="smpteC" value="10" />
        <enum name="adobeRgb" value="11" />
        <enum name="proPhotoRgb" value="12" />
        <enum name="aces" value="13" />
        <enum name="acescg" value="14" />
        <enum name="cieXyz" value="15" />
        <enum name="cieLab" value="16" />
        <enum name="bt2020Hlg" value="17" />
        <enum name="bt2020Pq" value="18" />
    </attr>
    <attr name="sketch_sizeWidth" format="dimension|reference" />
    <attr name="sketch_sizeHeight" format="dimension|reference" />
    <attr name="sketch_precision" format="enum">
        <enum name="exactly" value="1" />
        <enum name="sameAspectRatio" value="2" />
        <enum name="lessPixels" value="3" />
        <enum name="smallerSize" value="4" />
    </attr>
    <attr name="sketch_scale" format="enum">
        <enum name="startCrop" value="1" />
        <enum name="centerCrop" value="2" />
        <enum name="endCrop" value="3" />
        <enum name="fill" value="4" />
    </attr>
    <attr name="sketch_resizeOnDraw" format="boolean|reference" />
    <attr name="sketch_transformation" format="enum">
        <enum name="rotate" value="1" />
        <enum name="roundedCorners" value="2" />
        <enum name="circleCrop" value="3" />
        <enum name="blur" value="4" />
        <enum name="mask" value="5" />
    </attr>
    <attr name="sketch_transformation_rotate_degrees" format="integer|reference" />
    <attr name="sketch_transformation_roundedCorners_radius" format="dimension|reference" />
    <attr name="sketch_transformation_roundedCorners_radiusTopLeft" format="dimension|reference" />
    <attr name="sketch_transformation_roundedCorners_radiusTopRight" format="dimension|reference" />
    <attr name="sketch_transformation_roundedCorners_radiusBottomLeft" format="dimension|reference" />
    <attr name="sketch_transformation_roundedCorners_radiusBottomRight" format="dimension|reference" />
    <attr name="sketch_transformation_circleCrop_scale" format="enum">
        <enum name="startCrop" value="1" />
        <enum name="centerCrop" value="2" />
        <enum name="endCrop" value="3" />
        <enum name="fill" value="4" />
    </attr>
    <attr name="sketch_transformation_blur_radius" format="integer|reference" />
    <attr name="sketch_transformation_blur_hasAlphaBitmapBgColor" format="color|reference" />
    <attr name="sketch_transformation_blur_maskColor" format="color|reference" />
    <attr name="sketch_transformation_mask_maskColor" format="color|reference" />
    <attr name="sketch_resultCachePolicy" format="enum">
        <enum name="enabled" value="1" />
        <enum name="readOnly" value="2" />
        <enum name="writeOnly" value="3" />
        <enum name="disabled" value="4" />
    </attr>

    <attr name="sketch_memoryCachePolicy" format="enum">
        <enum name="enabled" value="1" />
        <enum name="readOnly" value="2" />
        <enum name="writeOnly" value="3" />
        <enum name="disabled" value="4" />
    </attr>
    <attr name="sketch_crossfade" format="boolean|reference" />
    <attr name="sketch_crossfadeDurationMillis" format="integer|reference" />
    <attr name="sketch_crossfadePreferExactIntrinsicSize" format="boolean|reference" />
    <attr name="sketch_crossfadeFadeStart" format="boolean|reference" />
    <attr name="sketch_crossfadeAlwaysUse" format="boolean|reference" />
    <attr name="sketch_placeholder" format="color|reference" />
    <attr name="sketch_fallback" format="color|reference" />
    <attr name="sketch_error" format="color|reference" />
    <attr name="sketch_src" format="color|reference" />

    <declare-styleable name="SketchImageView">
        <attr name="sketch_depth" />
        <attr name="sketch_downloadCachePolicy" />
        <attr name="sketch_preferQualityOverSpeed" />
        <attr name="sketch_colorType" />
        <attr name="sketch_colorSpace" />
        <attr name="sketch_sizeWidth" />
        <attr name="sketch_sizeHeight" />
        <attr name="sketch_precision" />
        <attr name="sketch_scale" />
        <attr name="sketch_resizeOnDraw" />
        <attr name="sketch_transformation" />
        <attr name="sketch_transformation_rotate_degrees" />
        <attr name="sketch_transformation_roundedCorners_radius" />
        <attr name="sketch_transformation_roundedCorners_radiusTopLeft" />
        <attr name="sketch_transformation_roundedCorners_radiusTopRight" />
        <attr name="sketch_transformation_roundedCorners_radiusBottomLeft" />
        <attr name="sketch_transformation_roundedCorners_radiusBottomRight" />
        <attr name="sketch_transformation_circleCrop_scale" />
        <attr name="sketch_transformation_blur_radius" />
        <attr name="sketch_transformation_blur_hasAlphaBitmapBgColor" />
        <attr name="sketch_transformation_blur_maskColor" />
        <attr name="sketch_transformation_mask_maskColor" />
        <attr name="sketch_resultCachePolicy" />
        <attr name="sketch_memoryCachePolicy" />
        <attr name="sketch_crossfade" />
        <attr name="sketch_crossfadeDurationMillis" />
        <attr name="sketch_crossfadePreferExactIntrinsicSize" />
        <attr name="sketch_crossfadeFadeStart" />
        <attr name="sketch_crossfadeAlwaysUse" />
        <attr name="sketch_placeholder" />
        <attr name="sketch_fallback" />
        <attr name="sketch_error" />
    </declare-styleable>
</resources>